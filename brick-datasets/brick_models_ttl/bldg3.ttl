@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix bldg: <http://buildsys.org/ontologies/bldg#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ref: <https://brickschema.org/schema/Brick/ref#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix ns4: <http://buildsys.org/ontologies/bldg#bldg.CHW.Pump1_Start/> .
@prefix ns5: <http://buildsys.org/ontologies/bldg#bldg.CHW.Pump2_Start/> .

bldg:AHU01 a brick:Air_Handler_Unit ;
    brick:hasPoint bldg:bldg3.AHU.AHU01.Cooling_Valve_Output,
        bldg:bldg3.AHU.AHU01.Heating_Valve_Output,
        bldg:bldg3.AHU.AHU01.Mixed_Air_Temp,
        bldg:bldg3.AHU.AHU01.Outside_Air_Temp,
        bldg:bldg3.AHU.AHU01.Supply_Air_Temp ;
    brick:isFedBy bldg:chiller .

bldg:bldg3 a brick:Building ;
    brick:area [ brick:hasUnits unit:FT_2 ;
            brick:value "42946" ] .

<urn:bldg3> a owl:Ontology ;
    rdfs:label "Mortar building bldg" ;
    owl:imports <https://brickschema.org/schema/1.4/Brick> .

bldg:bldg3.AHU.AHU01.Cooling_Valve_Output a brick:Cooling_Command,
        brick:Valve_Command ;
    rdfs:label "bldg.AHU.AHU01.Cooling Valve Output" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "e2d0c26e-835a-4531-8617-5b6694e89de6" ] .

bldg:bldg3.AHU.AHU01.Heating_Valve_Output a brick:Command ;
    rdfs:label "bldg.AHU.AHU01.Heating Valve Output" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "ed440447-9c6e-4608-b68a-09fa50a750fc" ] .

bldg:bldg3.AHU.AHU01.Mixed_Air_Temp a brick:Mixed_Air_Temperature_Sensor ;
    rdfs:label "bldg.AHU.AHU01.Mixed Air Temp" .

bldg:bldg3.AHU.AHU01.Outside_Air_Temp a brick:Outside_Air_Temperature_Sensor ;
    rdfs:label "bldg.AHU.AHU01.Outside Air Temp" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "a0818df1-34a0-4c4f-9987-b8775016f161" ] .

bldg:bldg3.AHU.AHU01.Supply_Air_Temp a brick:Supply_Air_Temperature_Sensor ;
    rdfs:label "bldg.AHU.AHU01.Supply Air Temp" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "79a64984-9051-4470-9454-6a74a52e5698" ] .

bldg:bldg3.CHW.CHWSSETPOINT a brick:Chilled_Water_Supply_Temperature_Setpoint ;
    rdfs:label "bldg.CHW.CHWSSETPOINT" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "82198451-bf5f-4c1a-945e-492bc7f407be" ] .

bldg:bldg3.CHW.ECONOMIZER a brick:Damper_Position_Command ;
    rdfs:label "bldg.CHW.ECONOMIZER" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "b4a4defa-503a-441e-ad3a-00da93bc5d3d" ] .

bldg:bldg3.CHW.Loop_Chilled_Water_Return_Temp a brick:Chilled_Water_Return_Temperature_Sensor ;
    rdfs:label "bldg.CHW.Loop Chilled Water Return Temp" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "4d2dd50e-9dc6-4a16-818b-19e794c4c6c4" ] .

bldg:bldg3.CHW.Loop_Chilled_Water_Return_Temp_Setpoint a brick:Chilled_Water_Return_Temperature_Setpoint ;
    rdfs:label "bldg.CHW.Loop Chilled Water Return Temp Setpoint" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "7c4fa671-d229-4147-8412-cdd2be6b2a45" ] .

bldg:bldg3.CHW.Loop_Chilled_Water_Supply_Temp a brick:Chilled_Water_Supply_Temperature_Sensor ;
    rdfs:label "bldg.CHW.Loop Chilled Water Supply Temp" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "5df38a23-8ea9-47ae-97cb-21ae87252318" ] .

bldg:bldg3.CHW.Loop_Chilled_Water_Supply_Temp_Setpoint a brick:Chilled_Water_Supply_Temperature_Setpoint ;
    rdfs:label "bldg.CHW.Loop Chilled Water Supply Temp Setpoint" ;
    ref:hasExternalReference [ ref:hasTimeseriesId "28434a2e-303f-4a82-99bf-a9387408574f" ] .

bldg:chiller a brick:Chiller ;
    brick:hasPoint bldg:bldg3.CHW.CHWSSETPOINT,
        bldg:bldg3.CHW.ECONOMIZER,
        bldg:bldg3.CHW.Loop_Chilled_Water_Return_Temp,
        bldg:bldg3.CHW.Loop_Chilled_Water_Return_Temp_Setpoint,
        bldg:bldg3.CHW.Loop_Chilled_Water_Supply_Temp,
        bldg:bldg3.CHW.Loop_Chilled_Water_Supply_Temp_Setpoint .

